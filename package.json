{
  "name": "react-starter",
  "version": "1.1.0",
  "description": "A starter for react project.",
  "repository": {
    "type": "git",
    "url": "git@github.com:lovelope/react-project.git"
  },
  "license": "MIT",
  "author": "lovelope <lovelope@qq.com>",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "build:dll": "cross-env NODE_ENV=production webpack --config tools/webpack.config.dll.babel.ts",
    "fix": "npm run fix-js && npm run lint-css",
    "fix-js": "npm run lint-js --fix",
    "format": "prettier --config prettier.config.js --write \"**/*.{ts,tsx,js,jsx,css,less,json,md,html}\"",
    "lint": "npm run lint-js && npm run lint-css",
    "lint-css": "stylelint \"src/**/*.{css,less}\" --syntax less",
    "lint-js": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "cross-env NODE_ENV=development webpack serve --color --progress"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-commit": "git update-index --again"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add --force"
    ],
    "*.{json,md,graphql}": [
      "prettier --write",
      "git add --force"
    ],
    "*.{css,less}": [
      "stylelint --syntax less",
      "git add --force"
    ]
  },
  "browserslist": {
    "production": [
      "last 2 Chrome versions",
      "Safari >= 9",
      "Firefox ESR",
      "Edge >= 12",
      "IE >= 11"
    ],
    "development": [
      "last 1 Chrome version",
      "Edge 17",
      "IE 11"
    ]
  },
  "dependencies": {
    "antd": "^4.14.0",
    "core-js": "^3.9.1",
    "dayjs": "^1.10.4",
    "history": "4.x",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "mobx": "^4.15.7",
    "mobx-react": "^6.3.1",
    "mockjs": "^1.1.0",
    "moment": "^2.29.1",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.6",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router": "^5.2.0",
    "regenerator-runtime": "^0.13.7",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.8",
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@mapbox/rehype-prism": "^0.5.0",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/mdx": "^1.6.22",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/webpack-dev-server": "^3.11.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.3",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "html-webpack-tags-plugin": "^3.0.0",
    "husky": "^4.3.6",
    "less": "^4.1.1",
    "less-loader": "^6.2.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.5.4",
    "lodash-webpack-plugin": "^0.11.6",
    "mini-css-extract-plugin": "^1.3.9",
    "postcss": "^8.2.8",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "terser-webpack-plugin": "^4.2.3",
    "typescript": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.25.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
