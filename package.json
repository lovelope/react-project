{
  "name": "react-starter",
  "version": "1.1.0",
  "description": "A starter for react project.",
  "repository": {
    "type": "git",
    "url": "git@github.com:lovelope/react-project.git"
  },
  "license": "MIT",
  "author": "lovelope <lovelope@qq.com>",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config tools/webpack.config.production.babel.js",
    "build:dll": "cross-env NODE_ENV=production webpack --config tools/webpack.config.dll.babel.js",
    "fix": "npm run fix-js && npm run lint-css",
    "fix-js": "npm run lint-js --fix",
    "format": "prettier --config prettier.config.js --write '**/*.{js,jsx,css,less,json}'",
    "lint": "npm run lint-js && npm run lint-css",
    "lint-css": "stylelint \"src/**/*.{less}\" --syntax less",
    "lint-js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" . --ext .js,.jsx,.ts,.tsx",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config tools/webpack.config.development.babel.js --color --progress"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-commit": "git update-index --again"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --no-ignore --fix",
      "git add --force"
    ],
    "*.{json,md,graphql}": [
      "prettier --write",
      "git add --force"
    ],
    "*.{css,less}": [
      "stylelint --syntax less",
      "git add --force"
    ]
  },
  "browserslist": {
    "production": [
      "last 2 Chrome versions",
      "Safari >= 9",
      "Firefox ESR",
      "Edge >= 12",
      "IE >= 11"
    ],
    "development": [
      "last 1 Chrome version",
      "Edge 17",
      "IE 11"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "antd": "^3.20.5",
    "core-js": "^2.6.9",
    "history": "^4.9.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.15",
    "mobx": "^4.13.0",
    "mobx-react": "^6.1.1",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.24.0",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@mapbox/rehype-prism": "^0.3.1",
    "@mdx-js/loader": "^1.1.0",
    "@mdx-js/mdx": "^1.1.0",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "^4.0.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-tags-plugin": "^2.0.13",
    "husky": "^3.0.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^9.2.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "progress-bar-webpack-plugin": "^1.12.1",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.1",
    "terser": "^4.1.2",
    "terser-webpack-plugin": "^1.3.0",
    "typescript": "^3.5.3",
    "url-loader": "^2.0.1",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
