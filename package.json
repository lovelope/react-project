    "@types/lodash": "4.x",
    "@types/node": "^17.0.33",
    "@types/react": "18.x",
    "@types/react-dom": "18.x",
    "@types/react-router": "^5.1.14",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.23.0",
    "antd-dayjs-webpack-plugin": "^1.0.6",
